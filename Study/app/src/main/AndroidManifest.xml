<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.study">

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Study">
        <activity android:name=".ControlKotlinActivity"
            android:label=".ControlKotlinActivity" />
        <activity android:name=".ControlJavaActivity"
            android:label=".ControlJavaActivity" />
        <activity
            android:name=".BmiKotlinActivity"
            android:label=".BmiKotlinActivity" />
        <activity
            android:name=".BmiJavaActivity"
            android:label=".BmiJavaActivity" />
        <activity android:name=".SecondActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>